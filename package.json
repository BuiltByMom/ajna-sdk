{
  "name": "@ajna-finance/sdk-api",
  "description": "A typescript SDK that can be used to create Dapps in Ajna ecosystem.",
  "version": "0.3.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/ajna-finance/sdk.git",
    "directory": "packages/sdk-api"
  },
  "keywords": [],
  "author": "@ajna-finance",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "build-lib:watch": "tsc-watch --onSuccess \"yalc push --sig\"",
    "prepublishOnly": "yarn build",
    "document": "typedoc src/index.ts",
    "test": "jest --silent=false --runInBand",
    "test:dev": "yarn test --watchAll",
    "typechain-abis": "typechain --target ethers-v5 --discriminate-types --out-dir src/types/contracts './src/abis/*.json'",
    "update-abis": "npx ts-node -e 'require(\"./src/utils/update-abis\").updateAbis();'"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "ethcall": "^4.8.13",
    "ethers": "^5.7.2",
    "expect": "^29.5.0",
    "lodash": "^4.17.21",
    "mathjs": "^11.8.1",
    "typedoc": "^0.24.8"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@typechain/ethers-v5": "^11.0.0",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jest": "~29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "dotenv": "^16.1.4",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "tslib": "^2.5.3",
    "typechain": "^8.2.0",
    "typescript": "^5.1.3"
  },
  "bugs": {
    "url": "https://github.com/ajna-finance/sdk/issues"
  },
  "homepage": "https://github.com/ajna-finance/sdk#readme"
}

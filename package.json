{
  "name": "@ajna-finance/sdk-api",
  "description": "A typescript SDK that can be used to create Dapps in Ajna ecosystem.",
  "version": "0.0.1",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "document": "typedoc src/index.ts",
    "test": "jest",
    "test:dev": "jest --watchAll",
    "test:with-output": "jest --silent=false",
    "typechain-abis": "typechain --target ethers-v5 --discriminate-types --out-dir src/types/contracts './src/abis/*.json'",
    "update-abis": "npx ts-node -e 'require(\"./src/utils/update-abis\").updateAbis();'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ajna-finance/sdk.git"
  },
  "keywords": [],
  "author": "@ajna-finance",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@metamask/detect-provider": "^2.0.0",
    "@metamask/providers": "^10.0.0",
    "ethcall": "^4.8.13",
    "ethers": "^5.7.2",
    "mathjs": "^11.5.1",
    "typedoc": "^0.24.1"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.2.0",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.8.4"
  },
  "bugs": {
    "url": "https://github.com/ajna-finance/sdk/issues"
  },
  "homepage": "https://github.com/ajna-finance/sdk#readme"
}
